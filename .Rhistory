as_pqnumber.default <- function(x, p = NULL, q = NULL){
y <- stringr::str_match(x, "0*(\\d+)\\.?(\\d+)?0*[eE]?([+-]?\\d+)?$")[1,-1]
pq <- if(!is.na(y[3])) as.numeric(y[3]) else 0
q <- nchar(y[1]) - 1L
p <- if(!is.na(y[2])) nchar(y[2]) else 0
nums <- rev(as.numeric(unlist(strsplit(na.omit(y[-3]), ""))))
pqnumber(1L, max(p, p-pq), max(q, q + pq),
c(integer(max(0, pq)), nums, integer(max(0, -pq))))
}
